{
  "version": 2,
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
